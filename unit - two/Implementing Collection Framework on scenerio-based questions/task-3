//Write a java program to store group of integers[order of insertion elements does not matters and store unique elements](Hashset)
//--------------------------------------------------------------------------------------------------------------------------------
//program:

package com.java.Collectionframeworks;
import java.util.HashSet;
import java.util.Iterator;
public class UniqueHashset {
public static void main(String[] args) {
HashSet<Integer> hs=new HashSet<>();   
	    hs.add(888);  
	    hs.add(500); 
	    hs.add(711);
	    hs.add(685); 
	    hs.add(300);
      Iterator<Integer> itr=hs.iterator();
      while(itr.hasNext()){
      System.out.println(itr.next());
}
}
}

//**********************************************************************************************************************************
//output:
//500
//711
//888
//300
//685

